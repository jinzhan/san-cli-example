{
  "name": "san-plugin-visualizer",
  "version": "1.0.0",
  "private": true,
  "description": "A San project",
  "scripts": {
    "start": "cross-env NODE_ENV=development san serve --config san.config.js",
    "build": "cross-env NODE_ENV=production san build --config san.config.js --stats",
    "analyzer": "cross-env NODE_ENV=production san build --config san.config.js --analyze",
    "build:modern": "cross-env NODE_ENV=production san build --config san.config.js --modern"
  },
  "keywords": [
    "San",
    "San CLI"
  ],
  "lint-staged": {
    "(scripts|src)/**/*.js": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "jinzhan",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@ecomfe/eslint-config": "^3.2.0",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "fecs": "^1.6.2",
    "hulk-mock-server": "^0.0.7",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "prettier": "^2.0.5",
    "san-cli": "^0.1.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "d3": "^3.5.6",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "san": "^3.8.1",
    "san-store": "1.1.0",
    "san-update": "^2.1.0"
  }
}
